# http 服务监听 ip
Host: "0.0.0.0"
# http 服务监听端口
Port: "9527"
# 当前环境（local、test 或 prod）
Env: "local"

# 全局日志
Log:
  Path: "logs/framework_%Y%m%d.log" # 全局日志路径（相对于项目的根目录）
  Level: "info"
  Format: "json"
  ReportCaller: false

